<!DOCTYPE MIME-resolver PUBLIC "-//NetBeans//DTD MIME Resolver 1.1//EN" "http://www.netbeans.org/dtds/mime-resolver-1_1.dtd">
<MIME-resolver>
    <file>
        <name name="string" ignorecase="true" substring="true"/>
        <ext name="xml"/>
        <resolver mime="text/x-android-strings+xml">
            <xml-rule>
                <element name="resources"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <name name="color" ignorecase="true" substring="true"/>
        <ext name="xml"/>
        <resolver mime="text/x-android-colors+xml">
            <xml-rule>
                <element name="resources"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <name name="dimen" ignorecase="true" substring="true"/>
        <ext name="xml"/>
        <resolver mime="text/x-android-dimens+xml">
            <xml-rule>
                <element name="resources"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <name name="style" ignorecase="true" substring="true"/>
        <ext name="xml"/>
        <resolver mime="text/x-android-styles+xml">
            <xml-rule>
                <element name="resources"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <name name="attr" ignorecase="true" substring="true"/>
        <ext name="xml"/>
        <resolver mime="text/x-android-attrs+xml">
            <xml-rule>
                <element name="resources"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <name name="array" ignorecase="true" substring="true"/>
        <ext name="xml"/>
        <resolver mime="text/x-android-array+xml">
            <xml-rule>
                <element name="resources"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <name name="bool" ignorecase="true" substring="true"/>
        <ext name="xml"/>
        <resolver mime="text/x-android-bool+xml">
            <xml-rule>
                <element name="resources"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <name name="integer" ignorecase="true" substring="true"/>
        <ext name="xml"/>
        <resolver mime="text/x-android-integer+xml">
            <xml-rule>
                <element name="resources"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <name name="id" ignorecase="true" substring="true"/>
        <ext name="xml"/>
        <resolver mime="text/x-android-ids+xml">
            <xml-rule>
                <element name="resources"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <name name="symbol" ignorecase="true" substring="true"/>
        <ext name="xml"/>
        <resolver mime="text/x-android-symbols+xml">
            <xml-rule>
                <element name="resources"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="xml"/>
        <pattern value="&lt;bool" range="4000" ignorecase="false"></pattern>
        <pattern value="&lt;color" range="4000" ignorecase="false"></pattern>
        <pattern value="&lt;dimen" range="4000" ignorecase="false"></pattern>
        <pattern value="&lt;item" range="4000" ignorecase="false"></pattern>
        <pattern value="&lt;integer" range="4000" ignorecase="false"></pattern>
        <pattern value="&lt;integer-array" range="4000" ignorecase="false"></pattern>
        <pattern value="&lt;array" range="4000" ignorecase="false"></pattern>
        <pattern value="&lt;string" range="4000" ignorecase="false"></pattern>
        <pattern value="&lt;string-array" range="4000" ignorecase="false"></pattern>
        <pattern value="&lt;plurals" range="4000" ignorecase="false"></pattern>
        <pattern value="&lt;style" range="4000" ignorecase="false"></pattern>
        <pattern value="&lt;declare-styleable" range="4000" ignorecase="false"></pattern>
        <pattern value="&lt;eat-comment" range="4000" ignorecase="false"></pattern>
        <pattern value="&lt;attr" range="4000" ignorecase="false"></pattern>
        <resolver mime="text/x-android-values+xml">
            <xml-rule>
                <element name="resources"/>
            </xml-rule>
            
        </resolver>
    </file>
</MIME-resolver>
