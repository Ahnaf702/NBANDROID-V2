                                <dependency>
                                    <!--Private Package Reference-->
                                    <id>org.netbeans.modules:org-netbeans-modules-gsf-testrunner</id>
                                    <type>impl</type>
                                </dependency>
                                <dependency>
                                    <!--Private Package Reference-->
                                    <id>com.github.kelemen:netbeans-gradle-plugin</id>
                                    <type>impl</type>
                                </dependency>
                            </moduleDependencies>
                        </configuration>
                    </plugin>
                </plugins>
            </build>
            <properties>
                <version.nb>RELEASE81</version.nb>
            </properties>
        </profile>
